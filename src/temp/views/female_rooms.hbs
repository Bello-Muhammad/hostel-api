<section data-bs-version="5.1" class="form8 cid-t5ZqMQe6Fe" id="form8-1m">



{{#if Name}}
    <div class="container">
        <div class="mbr-section-head">
            <h3 class="mbr-section-title mbr-fonts-style align-center mb-0 display-2">
                <strong>{{Name}} Hostel</strong>
            </h3>
            
        </div>
        <div class="row justify-content-center mt-4">
            {{#each room}}
            <div class="col-lg-4 mx-auto mbr-form" data-form-type="formoid">
                
                <form action="/reserve_amina" method="POST" class="mbr-form form-with-styler mx-auto" data-form-title="Form Name">
                    <div data-for="_id" class="col-lg-12 col-md-12 col-sm-12 form-group mb-3">
                        <input type="hidden" name="roomNumber" data-form-field="_id" class="form-control" value="{{this}}" id="phone-form7-17">
                    </div>                       
                    <div class="col-lg-4 col-md-12 col-sm-12 mbr-section-btn align-center">
                        <button type="submit" class="btn btn-primary display-4">Room {{this}}</button>
                    </div>
                </form>
                
            </div>
            {{/each}}
        </div>
    </div>
{{else}}
<div class="container">
        <div class="mbr-section-head">
            <h3 class="mbr-section-title mbr-fonts-style align-center mb-0 display-2">
                <strong>{{hostelName}} Hostel</strong>
            </h3>
            
        </div>
        <div class="row justify-content-center mt-4">
            {{#each room}}
            <div class="col-lg-4 mx-auto mbr-form" data-form-type="formoid">
                
                <form action="/reserve_khadija" method="POST" class="mbr-form form-with-styler mx-auto" data-form-title="Form Name">
                    <div data-for="_id" class="col-lg-12 col-md-12 col-sm-12 form-group mb-3">
                        <input type="hidden" name="roomNumber" data-form-field="_id" class="form-control" value="{{this}}" id="phone-form7-17">
                    </div>                       
                    <div class="col-lg-4 col-md-12 col-sm-12 mbr-section-btn align-center">
                        <button type="submit" class="btn btn-primary display-4">Room {{this}}</button>
                    </div>
                </form>
                
            </div>
            {{/each}}
        </div>
    </div>
{{/if}}
    

</section>


  
